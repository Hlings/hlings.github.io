      <geometry>
        <mesh filename="links/front_wheel_assembly.obj" />
      </geometry>
      <origin xyz="-2.25 -2.15 0.0" rpy="0.0 0.0 0.0" />
    </collision>
  </link>
  <link name="seat_saddle_assembly">
    <visual>
      <geometry>
        <mesh filename="links/seat_saddle_assembly.obj" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="links/seat_saddle_assembly.obj" />
      </geometry>
    </collision>
  </link>
  <link name="front_steering_assembly">
    <visual>
      <geometry>
        <mesh filename="links/front_steering_assembly.obj" />
      </geometry>
      <origin xyz="-1.05 -1.15 0.0" rpy="0.0 0.0 0.0" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="links/front_steering_assembly.obj" />
      </geometry>
      <origin xyz="-1.05 -1.15 0.0" rpy="0.0 0.0 0.0" />
