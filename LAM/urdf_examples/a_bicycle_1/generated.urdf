  </link>
  <link name="rear_wheel_assembly">
    <visual>
      <geometry>
        <mesh filename="links/rear_wheel_assembly.obj" />
      </geometry>
      <origin xyz="0.0 -1.0 0.0" rpy="0.0 0.0 0.0" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="links/rear_wheel_assembly.obj" />
      </geometry>
      <origin xyz="0.0 -1.0 0.0" rpy="0.0 0.0 0.0" />
    </collision>
  </link>
  <link name="bicycle_frame">
    <visual>
      <geometry>
        <mesh filename="links/bicycle_frame.obj" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="links/bicycle_frame.obj" />
      </geometry>
    </collision>
  </link>
  <joint name="frame_to_steering" type="revolute">
    <parent link="bicycle_frame" />
    <child link="front_steering_assembly" />
    <origin xyz="1.05 1.15 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <limit lower="-0.45" upper="0.45" effort="10" velocity="2" />
  </joint>
  <joint name="front_wheel_spin" type="continuous">
    <parent link="front_steering_assembly" />
    <child link="front_wheel_assembly" />
    <origin xyz="1.2 1.0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    
  </joint>
  <joint name="rear_wheel_spin" type="continuous">
    <parent link="bicycle_frame" />
    <child link="rear_wheel_assembly" />
    <origin xyz="0 1.0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    
  </joint>
  <joint name="crank_spin" type="revolute">
    <parent link="bicycle_frame" />
    <child link="crankset_pedals_assembly" />
    <origin xyz="0.3 0.45 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit lower="-1.57" upper="1.57" effort="10" velocity="5" />
  </joint>
  <joint name="frame_to_seat" type="fixed">
    <parent link="bicycle_frame" />
    <child link="seat_saddle_assembly" />
    
    
  </joint>
